REATE, UPDATE, DELETE } from '../constants/actionTypes';\n\nexport default (posts = [], action) => {\n    switch (action.type) {\n        case DELETE:\n            return posts.filter((post) => post._id !== action.payload);\n        case UPDATE:   \n            return posts.map((post) => post._id === action.payload._id ? action.payload : post);\n        case FETCH_ALL:\n            return action.payload; \n        case CREATE:\n            return [ ...posts, action.payload];\n        default:\n            return posts;\n    }\n}","/home/tushitayadav/MajorGroupProject/client/src/components/Posts/styles.js",[],"/home/tushitayadav/MajorGroupProject/client/src/constants/actionTypes.js",[],"/home/tushitayadav/MajorGroupProject/client/src/components/Form/styles.js",[],"/home/tushitayadav/MajorGroupProject/client/src/components/Posts/Post/Post.js",[],"/home/tushitayadav/MajorGroupProject/client/src/api/index.js",[],"/home/tushitayadav/MajorGroupProject/client/src/components/Posts/Post/styles.js",[],"/home/tushitayadav/MajorGroupProject/client/src/reportWebVitals.js",[],"/home/tushitayadav/MajorGroupProject/client/src/AdminPage.js",["152","153","154"],"import { React } from \"react\";\nimport reactDom from \"react-dom\";\nimport { Link, Switch, Route } from \"react-router-dom\";\nimport { Form, Container, Button, Navbar, Nav, FormControl } from \"react-bootstrap\";\nimport { LinkContainer } from \"react-router-bootstrap\";\nimport Issues from './components/Issues/Issues'\nimport Users from \"./components/Users/Users\";\nimport UserDetails from \"./components/Users/User/UserDetails\";\nimport UserEdit from \"./components/Users/User/UserEdit\";\n\n// This is the navigation bar that will be used for the admin's page\nconst NavigationBar = () => (\n    <Navbar bg=\"dark\" variant=\"dark\">\n         <Navbar.Collapse id=\"basic-navbar-nav\">\n        <Navbar.Brand>Admin</Navbar.Brand>\n        <Nav>\n            <Nav.Link>Statistics</Nav.Link>\n            <Nav.Link>Reports</Nav.Link>\n            <LinkContainer to=\"/Issues\">\n            <Nav.Link>Issues</Nav.Link>\n            </LinkContainer>\n            <LinkContainer to=\"/Users\">\n                <Nav.Link>Users</Nav.Link>\n            </LinkContainer>\n        </Nav>\n        <Navbar.Toggle />\n        <Form inline>\n            <FormControl type=\"text\" placeholder=\"\" />\n            <Button variant=\"outline-info\">Search</Button>\n        </Form>\n        </Navbar.Collapse>\n    </Navbar>\n);\n\nconst style = {\n    margin: \"33px auto 36px\",\n    padding: \"35px\",\n};\n\nconst AdminPage = () => (\n    <div>\n        <NavigationBar />\n        <br />\n        <Container>\n            <Switch>\n                <Route exact path='/users' component={Users}/>\n                <Route exact path='/users/:id' component={UserDetails}/>\n                <Route exact path='/users/edit/:id' component={UserEdit}/>\n                <Route exact path='/issues' component={Issues}/>\n            </Switch>\n        </Container>\n    </div>\n);\n\nexport default AdminPage;","/home/tushitayadav/MajorGroupProject/client/src/Main.js",[],"/home/tushitayadav/MajorGroupProject/client/src/actions/issues.js",[],"/home/tushitayadav/MajorGroupProject/client/src/components/Users/Users.js",["155","156"],"import { React, useEffect } from \"react\";\nimport reactDom from \"react-dom\";\nimport { Link } from \"react-router-dom\";\nimport { Table } from \"react-bootstrap\";\nimport { useSelector, useDispatch } from \"react-redux\";\nimport { getUsers } from \"../../actions/users\";\nimport User from \"./User/User\";\nimport UserForm from './UserForm'\n\nconst Users = () => {\n\n    const dispatch = useDispatch();\n\n    useEffect(() => {\n      dispatch(getUsers());\n    }, [dispatch]);\n\n    const users = useSelector((state) => state.users);\n\n    return (\n        <>\n        <UserForm />\n        <Table>\n                <thead>\n                    <tr>\n                        <th>ID</th>\n                        <th>Username</th>\n                        <th>Email</th>\n                        <th>View</th>\n                    </tr>\n                </thead>\n                <tbody>\n                    {users.map((user) => (\n                        <tr key={user._id} >\n                            <User user={user} />\n                        </tr>\n                    ))}\n                </tbody>\n            </Table>\n        </>\n    );\n}\n\nexport default Users;","/home/tushitayadav/MajorGroupProject/client/src/AdminLogin.js",["157","158","159"],"import { useFormik } from \"formik\";\nimport { React } from \"react\";\nimport reactDom from \"react-dom\";\nimport { Link, Redirect, useHistory } from \"react-router-dom\";\nimport { Form, Container, Button } from \"react-bootstrap\";\nimport * as Yup from \"yup\";\n\n\n//This is the style of the login box\nconst loginStyle = {\n    margin: \"33px auto 36px\",\n    maxWidth: \"550px\",\n    padding: \"35px\",\n};\n\n//This the the main page it has the main container and the login box.\nconst AdminLogin = () => (\n    <Container className=\"border border-secondary\" style={loginStyle}>\n    <Login />\n    </Container>\n);\n\n//A schema to make sure that all entered details' requirements are met\nconst adminSchema = Yup.object().shape({\n    username: Yup.string()\n        .required(),\n    \n    password: Yup.string()\n        .required()\n});\n\n// The login form. It controls how the data is managed, and it returns the form\n// that will be displayed.\nconst Login = () => {\n\n\n    const history = useHistory();\n\n    const reroute = () => {\n        let newPage = 'AdminPage';\n        history.push(newPage);\n    }\n\n    const Formik = useFormik({\n        initialValues: {\n            username: \"\",\n            password: \"\"\n        },\n        validationSchema: adminSchema,\n        onSubmit(values) {\n            console.log(\"Log in details: \", values);\n            reroute();\n        }\n    });\n\n    return(\n        <Form onSubmit={Formik.handleSubmit} >\n            <Form.Group>\n            <Form.Label>Admin Username</Form.Label>\n            <Form.Control\n                id=\"username\"\n                name=\"username\"\n                type=\"text\"\n                placeholder=\"Enter Username\"\n                value={Formik.values.username}\n                onChange={Formik.handleChange}\n            />\n            </Form.Group>\n\n            <Form.Group>\n            <Form.Label>Admin Password</Form.Label>\n            <Form.Control\n                id=\"password\"\n                name=\"password\"\n                type=\"password\" \n                placeholder=\"Password\"\n                value={Formik.values.password}\n                onChange={Formik.handleChange}\n            />\n            </Form.Group>\n\n            <Button variant=\"primary\" type=\"submit\">Login</Button>\n        </Form>\n    )\n\n}\n\n\n\nexport default AdminLogin;","/home/tushitayadav/MajorGroupProject/client/src/LandingPage.js",["160"],"import React from \"react\";\nimport { Button , Alert, Container, Carousel } from 'react-bootstrap';\nimport 'bootstrap/dist/css/bootstrap.min.css';\nimport './LandingPage.css';\n\nfunction LandingPage () {\n    return (\n        <div className = 'landingPage'> \n            <body className = 'body'>   \n\n                <Alert className = 'alert1'> Welcome to our Fitness and Nutrition Aggregator !  </Alert>\n                <Alert className = 'alert2'> Please precise us if you are a professional user or not. </Alert>\n\n                <div class=\"row justify-content-center\"\n                >     \n                    <Button className=\"button\" variant=\"outline-success\"> Basic User </Button>\n                    <Button className=\"button\" variant=\"outline-success\" > Professional</Button>\n                </div>    \n\n                <Carousel className =\"carousel\">\n                        <Carousel.Item>\n