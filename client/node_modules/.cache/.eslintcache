iption}\n            placeholder=\"Enter Description\"\n            onChange={(e) => setIssueData( { ...IssueData, description: e.target.value } )}\n        />\n        </Form.Group>\n\n        <Button variant=\"primary\" type=\"submit\">Create</Button>\n    </Form>\n    );\n}\n\nexport default IssueForm;","/Users/khalid-alsheeb/5CCS2SEG/MajorGroupProject/client/src/components/Issues/Issue/Issue.js",[],"/Users/khalid-alsheeb/5CCS2SEG/MajorGroupProject/client/src/reducers/issues.js",["106"],"export default (issues = [], action) => {\n    switch (action.type) {\n        case 'DELETE_ISSUE':\n            return issues.filter((issue) => issue._id !== action.payload);\n        case 'UPDATE_ISSUE': \n        return issues.map((issue) => issue._id === action.payload._id ? action.payload : issue);\n        case 'FETCH_ALL_ISSUES':\n            return action.payload;\n        case 'CREATE_ISSUE':\n            return [ ...issues, action.payload ];\n        default:\n            return issues;\n    }\n}",{"ruleId":"107","replacedBy":"108"},{"ruleId":"109","replacedBy":"110"},{"ruleId":"111","severity":1,"message":"112","line":4,"column":8,"nodeType":"113","messageId":"114","endLine":4,"endColumn":13},{"ruleId":"111","severity":1,"message":"115","line":5,"column":8,"nodeType":"113","messageId":"114","endLine":5,"endColumn":17},{"ruleId":"111","severity":1,"message":"116","line":6,"column":10,"nodeType":"113","messageId":"114","endLine":6,"endColumn":19},{"ruleId":"111","severity":1,"message":"117","line":8,"column":7,"nodeType":"113","messageId":"114","endLine":8,"endColumn":17},{"ruleId":"111","severity":1,"message":"118","line":5,"column":8,"nodeType":"113","messageId":"114","endLine":5,"endColumn":14},{"ruleId":"111","severity":1,"message":"112","line":6,"column":8,"nodeType":"113","messageId":"114","endLine":6,"endColumn":13},{"ruleId":"119","severity":1,"message":"120","line":1,"column":1,"nodeType":"121","endLine":14,"endColumn":2},{"ruleId":"111","severity":1,"message":"