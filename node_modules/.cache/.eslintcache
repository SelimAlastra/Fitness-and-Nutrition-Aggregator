Visiblity}>{eye}</i>\n    {formik.errors.retypePassword && formik.touched.retypePassword && (\n    <div style={{color: \"red\"}} className=\"input-feedback\">{formik.errors.retypePassword}</div>\n    )}\n    <p/>\n  \n    <Button type=\"submit\" disabled={formik.isSubmitting}>\n        Register\n    </Button>\n</Form>\n);\n};\n\nexport default SignUp;","/Users/ioanaghinea/major-group-project/MajorGroupProject/src/client_signup.js",["40","41"],"import React, { Component, useState} from 'react';\nimport { Link } from 'react-router-dom';\nimport Button from 'react-bootstrap/Button';\nimport 'bootstrap/dist/css/bootstrap.min.css';\n\nfunction SignUpButton(){\n    return(\n        <Link to=\"/register_form\">\n            <Button>\n              Register\n            </Button>\n        </Link>\n  );\n};\n\nexport default SignUpButton;",{"ruleId":"42","severity":1,"message":"43","line":4,"column":8,"nodeType":"44","messageId":"45","endLine":4,"endColumn":11},{"ruleId":"46","replacedBy":"47"},{"ruleId":"48","replacedBy":"49"},{"ruleId":"42","severity":1,"message":"50","line":1,"column":8,"nodeType":"44","messageId":"45","endLine":1,"endColumn":12},{"ruleId":"42","severity":1,"message":"51","line":1,"column":17,"nodeType":"44","messageId":"45","endLine":1,"endColumn":26},{"ruleId":"42","severity":1,"message":"52","line":2,"column":10,"nodeType":"44","messageId":"45","endLine":2,"endColumn":14},{"ruleId":"42","severity":1,"message":"53","line":4,"column":13,"nodeType":"44","messageId":"45","endLine":4,"endColumn":27},{"ruleId":"42","severity":1,"message":"54","line":8,"column":8,"nodeType":"44","messageId":"45","endLine":8,"endColumn":13},{"ruleId":"55","severity":1,"message":"56","line":28,"column":33,"nodeType":"57","messageId":"58","endLine":28,"endColumn":34,"suggestions":"59"},{"ruleId":"55","severity":1,"message":"60","line":28,"column":36,"nodeType":"57","messageId":"58","endLine":28,"endColumn":37,"suggestions":"61"},{"ruleId":"55","severity":1,"message":"62","line":